{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "title1": {
      "type": "string",
      "required": true
    },
    "subtitle1": {
      "type": "string",
      "required": true
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text",
      "required": true
    },
    "title2": {
      "type": "string",
      "required": true
    },
    "title3": {
      "type": "string",
      "required": true
    },
    "video": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "videos"
      ]
    },
    "brands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service.service",
      "inversedBy": "products"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "price": {
      "type": "decimal"
    },
    "tax": {
      "type": "decimal"
    },
    "newDescription": {
      "type": "richtext",
      "required": true
    }
  }
}
