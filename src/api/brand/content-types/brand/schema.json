{
  "kind": "collectionType",
  "collectionName": "brands",
  "info": {
    "singularName": "brand",
    "pluralName": "brands",
    "displayName": "Brand"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "brands"
    }
  }
}
